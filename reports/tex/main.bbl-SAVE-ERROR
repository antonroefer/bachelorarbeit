      \strng{namehash}{9894226fef9e4a085d4731265f72dd3d}
      \strng{fullhash}{d34b370df11b457e064bf0965b30a186}
      \strng{fullhashraw}{d34b370df11b457e064bf0965b30a186}
      \strng{bibnamehash}{d34b370df11b457e064bf0965b30a186}
